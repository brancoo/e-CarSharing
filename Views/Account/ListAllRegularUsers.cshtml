@model IEnumerable<e_CarSharing.Models.RegularUser>

@{
    ViewBag.Title = "ListAllRegularUsers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ListAllRegularUsers</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RegularUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RegularUserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.RegularUserId }) |
                @Html.ActionLink("Delete", "DeleteRegularUser", new { id = item.RegularUserId })
            </td>
        </tr>
    }

</table>
